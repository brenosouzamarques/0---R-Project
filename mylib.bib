@article{korotkovaNMDAReceptorAblation2010,
  title = {{{NMDA Receptor Ablation}} on {{Parvalbumin-Positive Interneurons Impairs Hippocampal Synchrony}}, {{Spatial Representations}}, and {{Working Memory}}},
