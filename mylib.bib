  title = {Antidepressant Effects of Ketamine in Depressed Patients},
  author = {Berman, Robert M. and Cappiello, Angela and Anand, Amit and Oren, Dan A. and Heninger, George R. and Charney, Dennis S. and Krystal, John H.},
