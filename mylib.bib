@article{disnerNeuralMechanismsCognitive2011,
  title = {Neural Mechanisms of the Cognitive Model of Depression},
  author = {Disner, Seth G. and Beevers, Christopher G. and Haigh, Emily A. P. and Beck, Aaron T.},
