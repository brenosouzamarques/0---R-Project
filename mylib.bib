@article{murroughNeurocognitiveEffectsKetamine2014,
  title = {Neurocognitive Effects of Ketamine in Treatment-Resistant Major Depression: Association with Antidepressant Response},
  shorttitle = {Neurocognitive Effects of Ketamine in Treatment-Resistant Major Depression},
  author = {Murrough, James W. and Wan, Le-Ben and Iacoviello, Brian and Collins, Katherine A. and Solon, Carly and Glicksberg, Benjamin and Perez, Andrew M. and Mathew, Sanjay J. and Charney, Dennis S. and Iosifescu, Dan V. and Burdick, Katherine E.},
  date = {2014-02},
  journaltitle = {Psychopharmacology},
  shortjournal = {Psychopharmacology},
  volume = {231},
